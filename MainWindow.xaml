<Window x:Class="AlbionOnlineCraftingCalculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AlbionOnlineCraftingCalculator"
        mc:Ignorable="d"
        Title="MainWindow" Height="1040" Width="1920" Loaded="OnLoad" Closing="Window_Closing" WindowState="Maximized"
    xmlns:theme="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero2">

    <Window.Resources>
        <Style x:Key="TextStyle" TargetType="{x:Type FrameworkElement}">
            <Setter Property="Control.Foreground" Value="White"/>
        </Style>

        <Style TargetType="TextBlock" BasedOn="{StaticResource TextStyle}"/>

        <Style  TargetType="{x:Type ComboBox}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ComboBox">
                        <Grid>
                            <ToggleButton Grid.Column="2" Focusable="false" IsChecked="{Binding Path=IsDropDownOpen,Mode=TwoWay,RelativeSource={RelativeSource TemplatedParent}}" >
                                <ToggleButton.Template>
                                    <ControlTemplate>

                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="5*" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>
                                            <Border x:Name="Border"  Grid.ColumnSpan="2" CornerRadius="5" Background="#CD8C57" BorderBrush="Black" BorderThickness="1" />
                                            <Border Grid.Column="0" CornerRadius="5,0,0,5"  Margin="1"  Background="Transparent"  BorderBrush="Black" BorderThickness="0,0,1,0" />
                                            <Path x:Name="Arrow" Grid.Column="1"  Fill="#CD8C57" HorizontalAlignment="Center" VerticalAlignment="Center" Data="M 0 0 L 4 4 L 8 0 Z"/>
                                        </Grid>

                                        <ControlTemplate.Triggers>
                                            <Trigger Property="ToggleButton.IsMouseOver" Value="true">
                                                <Setter TargetName="Border" Property="Background" Value="Transparent" />
                                            </Trigger>
                                            <Trigger Property="ToggleButton.IsChecked" Value="true">
                                                <Setter TargetName="Border" Property="Background" Value="Transparent" />
                                            </Trigger>
                                        </ControlTemplate.Triggers>
                                    </ControlTemplate>
                                </ToggleButton.Template>
                            </ToggleButton>
                            <ContentPresenter Name="ContentSite" IsHitTestVisible="False"  Content="{TemplateBinding SelectionBoxItem}" ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}" ContentTemplateSelector="{TemplateBinding ItemTemplateSelector}" Margin="3" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            <TextBox x:Name="PART_EditableTextBox" Visibility="Hidden" IsReadOnly="{TemplateBinding IsReadOnly}"/>
                            <Popup Name="Popup" Placement="Bottom" IsOpen="{TemplateBinding IsDropDownOpen}" AllowsTransparency="True"  Focusable="False" PopupAnimation="Slide">
                                <Grid  Name="DropDown" SnapsToDevicePixels="True" MinWidth="{TemplateBinding ActualWidth}" MaxHeight="{TemplateBinding MaxDropDownHeight}">
                                    <Border x:Name="DropDownBorder" Background="Blue" />
                                    <ScrollViewer SnapsToDevicePixels="True">
                                        <StackPanel IsItemsHost="True" />
                                    </ScrollViewer>
                                </Grid>
                            </Popup>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>

            <Setter Property="Background" Value="#1E2126"/>
            <Setter Property="BorderBrush" Value="#1E2126"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>


        </Style>


        <Style x:Key="ListBoxItemStyle1" TargetType="{x:Type ListBoxItem}">
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="HorizontalContentAlignment" Value="{Binding HorizontalContentAlignment, 
            RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}"/>
            <Setter Property="VerticalContentAlignment" Value="{Binding VerticalContentAlignment,
             RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}"/>
            <Setter Property="Padding" Value="2,0,0,0"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ListBoxItem}">
                        <Border x:Name="Bd" BorderBrush="{TemplateBinding BorderBrush}" 
                            BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" 
                            Padding="{TemplateBinding Padding}" SnapsToDevicePixels="true">
                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" 
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" 
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsSelected" Value="true">
                                <Setter Property="Background" TargetName="Bd" Value="Transparent"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <Style  TargetType="{x:Type ComboBoxItem}">
            <Setter Property="Background" Value="#1E2126"/>
            <Setter Property="BorderBrush" Value="#1E2126"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>

        </Style>

        <DataTemplate x:Key="itemTemplateV2">


            <Grid ShowGridLines="{Binding Path=ShowGridLines}" Grid.Row="1" MaxHeight="120" Background="{Binding Path=BackgroundColor}" >


                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="0.1*"/>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="2.2*"/>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="1.8*"/>
                    <ColumnDefinition Width="0.1*"/>

                </Grid.ColumnDefinitions>






                <!-- Colum1    Item Picture, Item Name , Item Price -->
                <Grid ShowGridLines="{Binding Path=ShowGridLines}"  Grid.Column="1" Margin="2" Name="Grid_item" Background="{Binding Path=BackgroundColor}" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>

                    <Image Name="Img_item" Grid.Row="0" Grid.Column="0" VerticalAlignment="Center" Source="{Binding Path=EndProductImage}"></Image>
                    <TextBlock Foreground="White" Name="Tb_item_Name" Grid.Row="0" Grid.Column="1"  VerticalAlignment="Top" Margin="0,35,0,0" Text="{Binding Path=EndProductName}" ></TextBlock>
                    <TextBlock Foreground="White" Name="Tb_item_Price"  Grid.Row="0" Grid.Column="1" VerticalAlignment="Bottom" Margin="0,0,0,35" Text="{Binding Path=EndProductPrice}" ></TextBlock>
                </Grid>



                <!-- Colum2 Ingredient1,2,3,4 -->
                <!--  -->
                <Grid ShowGridLines="{Binding Path=ShowGridLines}"  Grid.Column="2">

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="1*"></ColumnDefinition>

                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="1*"></RowDefinition>
                        <RowDefinition Height="1*"></RowDefinition>

                    </Grid.RowDefinitions>






                    <Grid ShowGridLines="{Binding Path=ShowGridLines}" Grid.Row="0" Grid.Column="0" Margin="2" Name="Grid_item_1" Background="{Binding Path=BackgroundColor}" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="4*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>


                        <Image Name="Img_item_Ing1" MaxHeight="50" MaxWidth="50" VerticalAlignment="Center" Grid.Column="0"  Source="{Binding Ingredients[0].IngredientImage}"></Image>

                        <TextBlock Foreground="White" Name="Tb_item_Ing1" VerticalAlignment="Top" Grid.Column="1" Margin="0,5,0,0" Text="{Binding Ingredients[0].IngredientName}"></TextBlock>
                        <TextBlock Foreground="White" Name="Tb_item_Ing1_Price" VerticalAlignment="Bottom" Grid.Column="1" Margin="0,0,0,5" Text="{Binding Ingredients[0].IngredientPrice}"></TextBlock>

                    </Grid>




                    <Grid ShowGridLines="{Binding Path=ShowGridLines}" Grid.Row="1" Grid.Column="0" Margin="2" Name="Grid_item_2" Background="{Binding Path=BackgroundColor}" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="4*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>


                        <Image Name="Img_item_Ing2" MaxHeight="50" MaxWidth="50" VerticalAlignment="Center" Grid.Column="0" Source="{Binding Path=Ingredients[1].IngredientImage}"></Image>

                        <TextBlock Foreground="White" Name="Tb_item_Ing2" VerticalAlignment="Top" Grid.Column="1" Margin="0,5,0,0" Text="{Binding Path=Ingredients[1].IngredientName}"></TextBlock>
                        <TextBlock Foreground="White" Name="Tb_item_Ing2_Price" VerticalAlignment="Bottom" Grid.Column="1" Margin="0,0,0,5" Text="{Binding Path=Ingredients[1].IngredientPrice}"></TextBlock>

                    </Grid>

                    <Grid ShowGridLines="{Binding Path=ShowGridLines}" Grid.Row="0" Grid.Column="1" Margin="2" Name="Grid_item_3" Background="{Binding Path=BackgroundColor}" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="4*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>


                        <Image Name="Img_item_Ing3" MaxHeight="50" MaxWidth="50" VerticalAlignment="Center" Grid.Column="0" Source="{Binding Path=Ingredients[2].IngredientImage}"></Image>

                        <TextBlock Foreground="White" Name="Tb_item_Ing3" VerticalAlignment="Top" Grid.Column="1" Margin="0,5,0,0" Text="{Binding Path=Ingredients[2].IngredientName}"></TextBlock>
                        <TextBlock Foreground="White" Name="Tb_item_Ing3_Price" VerticalAlignment="Bottom" Grid.Column="1" Margin="0,0,0,5" Text="{Binding Path=Ingredients[2].IngredientPrice}"></TextBlock>

                    </Grid>

                    <Grid ShowGridLines="{Binding Path=ShowGridLines}" Grid.Row="1" Grid.Column="1" Margin="2" Name="Grid_item_5" Background="{Binding Path=BackgroundColor}" Visibility="Visible" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="4*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>


                        <Image Name="Img_item_Ing4" MaxHeight="50" MaxWidth="50" VerticalAlignment="Center" Grid.Column="0" Source="{Binding Path=Ingredients[3].IngredientImage}"></Image>

                        <TextBlock Foreground="White" Name="Tb_item_Ing4" VerticalAlignment="Top" Grid.Column="1" Margin="0,5,0,0" Text="{Binding Path=Ingredients[3].IngredientName}"></TextBlock>
                        <TextBlock Foreground="White" Name="Tb_item_Ing4_Price" VerticalAlignment="Bottom" Grid.Column="1" Margin="0,0,0,5" Text="{Binding Path=Ingredients[3].IngredientPrice}"></TextBlock>

                    </Grid>








                </Grid>

                <!-- End of Ingredients -->


                <!-- Column 3 Spec Input, Focus Cost , Nutrition Tax , Sell order Tax -->
                <!--  -->
                <Grid ShowGridLines="{Binding Path=ShowGridLines}"  Grid.Column="3"   >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="1*"></RowDefinition>
                        <RowDefinition Height="1*"></RowDefinition>
                    </Grid.RowDefinitions>

                    <Grid ShowGridLines="{Binding Path=ShowGridLines}" Grid.Column="0" Grid.Row="0"  Name="Grid_item_Focus" Visibility="{Binding Path=Focus.Visible}" Margin="2" >

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="2*"></ColumnDefinition>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="1*"></ColumnDefinition>


                        </Grid.ColumnDefinitions>


                        <TextBlock Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,5,0,0" Grid.Row="0" Grid.Column="0" Visibility="{Binding Focus.Visible}" >Spec</TextBlock>
                        <TextBox IsEnabled="{Binding Focus.IsEnabled}"  Visibility="{Binding Focus.Visible}" Text="{Binding Focus.CraftingSpec}" Name="Tb_item_CraftingSpec" Margin="0,0,0,5" VerticalAlignment="Bottom" Grid.Row="0" TextAlignment="Center"  Grid.Column="0"  TextChanged="Tb_Item_CraftingSpec_TextChanged" PreviewTextInput="Tb_Item_CraftingSpec_PreviewTextInput" MaxLength="3" MaxLines="1" Height="21" Width="30"  HorizontalAlignment="Center"   ></TextBox>
                        <TextBlock  Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,5,0,0" Grid.Row="0" Grid.Column="1"  Visibility="{Binding Focus.Visible}" >Focus Cost</TextBlock>
                        <TextBlock Foreground="White" Text="{Binding Focus.FocusCost}" Name="Tb_item_FocusCost" VerticalAlignment="Bottom" Margin="0,0,0,7" Grid.Row="0" Grid.Column="1" MaxHeight="20" MaxWidth="30"  Visibility="{Binding Path=Focus.Visible}"></TextBlock>

                    </Grid>


                    <Grid ShowGridLines="{Binding Path=ShowGridLines}" Grid.Row="1" Grid.Column="0"  Margin="2" Name="Grid_item_Nutrition_Tax" Visibility="Visible" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*"></ColumnDefinition>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>

                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"></RowDefinition>
                            <RowDefinition Height="1*"></RowDefinition>
                        </Grid.RowDefinitions>

                        <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal">
                            <TextBlock Foreground="White" Visibility="{Binding Focus.Visible}" >Sell Order Tax</TextBlock>
                            <TextBlock Foreground="White"  Name="Tb_item_SellOrderTax" Text="{Binding Path=Extras.SellOrderTax}" Margin="2,0,0,0" Visibility="{Binding Focus.Visible}"></TextBlock>
                        </StackPanel>


                        <StackPanel Grid.Row="1" Grid.Column="0" Orientation="Horizontal">
                            <TextBlock Foreground="White" Visibility="{Binding Focus.Visible}" >Nutrition Cost</TextBlock>
                            <TextBlock Foreground="White"  Name="Tb_item_NutritionCost" Text="{Binding Path=Extras.NutritionTax}" Margin="2,0,0,0" Visibility="{Binding Focus.Visible}"></TextBlock>
                        </StackPanel>
                    </Grid>






                </Grid>








                <!-- Column 4 Journal Image, Journal Full,Empty Price, Filled Percentage -->
                <Grid ShowGridLines="{Binding Path=ShowGridLines}"   Grid.Column="4"  Margin="0,2">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="1*"></RowDefinition>
                        <RowDefinition Height="1*"></RowDefinition>
                    </Grid.RowDefinitions>




                    <Grid  ShowGridLines="{Binding Path=ShowGridLines}" Grid.Row="0" Grid.Column="0" Margin="2" Name="Grid_item_4" Background="Transparent" Visibility="{Binding Journal.Visible}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="4*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>


                        <Image Name="Img_item_Journal" MaxHeight="50" MaxWidth="50" VerticalAlignment="Center" Grid.Column="0" Margin="0,5,0,5" Visibility="{Binding Journal.Visible}" Source="{Binding Path=Journal.JournalImage}"></Image>
                        <StackPanel Orientation="Vertical" Grid.Column="1">
                            <TextBlock Visibility="{Binding Journal.Visible}" Foreground="White" Name="Tb_item_Journal_Full_Price"  Text="{Binding Path=Journal.JournalFullPrice}" ></TextBlock>
                            <TextBlock Visibility="{Binding Journal.Visible}" Foreground="White" Name="Tb_item_Journal_Empty_Price"   Text="{Binding Path=Journal.JournalEmptyPrice}" ></TextBlock>
                            <TextBlock Visibility="{Binding Journal.Visible}" Foreground="White" Name="Tb_item_Journal_FilledPercentage" Text="{Binding Path=Journal.JournalFilledPercentage}"  ></TextBlock>
                        </StackPanel>
                    </Grid>


                    <Grid ShowGridLines="{Binding Path=ShowGridLines}" Grid.Row="0" Grid.Column="1" Margin="2" Background="Transparent" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="4*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>


                        <Image Name="Img_Profit_Yes_No" MaxHeight="50" MaxWidth="50" VerticalAlignment="Center" Grid.Column="0" Margin="0,5,0,5" Source="{Binding Path=Profit.ProfitImage}"></Image>
                        <StackPanel Orientation="Vertical" Grid.Column="1">
                            <TextBlock Foreground="White" Name="Tb_Profit_In"  Text="{Binding Path=Profit.Investment}" ></TextBlock>
                            <TextBlock Foreground="White" Name="Tb_Profit_Out"   Text="{Binding Path=Profit.Returned}" ></TextBlock>
                            <TextBlock Foreground="White" Name="Tb_Profit_Unknown" Text="{Binding Path=Profit.unknown}"  ></TextBlock>
                        </StackPanel>


                    </Grid>






                </Grid>

                <!-- Column 5 -->
                <Grid ShowGridLines="{Binding Path=ShowGridLines}"  Grid.Column="5"  Margin="0,2">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="1*"></RowDefinition>
                        <RowDefinition Height="1*"></RowDefinition>
                    </Grid.RowDefinitions>



                </Grid>




            </Grid>



        </DataTemplate>



       


       
        <!--<Style TargetType="{x:Type ListBoxItem}">
            
        </Style>-->


    </Window.Resources>
    <Grid Background="#1E2126">




        <Grid.RowDefinitions>

            <RowDefinition Height="100"/>
            <RowDefinition Height="*"/>

        </Grid.RowDefinitions>

        <Grid Grid.Row="0" >



            <Grid.RowDefinitions>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
            </Grid.RowDefinitions>

            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.3*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="0.2*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="0.3*"/>

            </Grid.ColumnDefinitions>





            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" Grid.Column="1" Grid.Row="1" > Category</TextBlock>
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center"  FontSize="12" Grid.Column="2" Grid.Row="1" > SubCategory</TextBlock>
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center"  FontSize="12" Grid.Column="3" Grid.Row="1">Tier</TextBlock>
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center"  FontSize="12" Grid.Column="4" Grid.Row="1">Enchantment</TextBlock>
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center"  FontSize="12" Grid.Column="5" Grid.Row="1">Selling Location</TextBlock>
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center"  FontSize="12" Grid.Column="6" Grid.Row="1">Buying Location</TextBlock>

            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" Grid.Column="7" Grid.Row="1">Return Rate</TextBlock>



            <ComboBox   Grid.Row="2" Grid.Column="1" Name="cmb_cat" SelectionChanged="cmb_SelectionChanged" Foreground="White" Background="#1E2126" >



            </ComboBox>
            <ComboBox   Grid.Row="2" Grid.Column="2" Name ="cmb_subcat" SelectionChanged="cmb_SelectionChanged" Foreground="White" Background="#1E2126">

            </ComboBox>
            <ComboBox  Grid.Row="2" Grid.Column="3" Name="cmb_tier" SelectionChanged="cmb_SelectionChanged" Foreground="White" Background="#1E2126">

            </ComboBox>


            <ComboBox   Grid.Row="2" Grid.Column="4" Name="cmb_enchantment" SelectionChanged="cmb_SelectionChanged" Foreground="White" Background="#1E2126">

            </ComboBox>

            <ComboBox   Grid.Row="2" Grid.Column="5" Name="cmb_selling_location" SelectionChanged="cmb_SelectionChanged" Foreground="White" Background="#1E2126">

            </ComboBox>

            <ComboBox   Grid.Row="2" Grid.Column="6" Name="cmb_buying_location" SelectionChanged="cmb_SelectionChanged" Foreground="White" Background="#1E2126">

            </ComboBox>

            <ComboBox   Grid.Row="2" Grid.Column="7" Name="cmb_returnrate" SelectionChanged="cmb_SelectionChanged" Foreground="White" Background="#1E2126">

            </ComboBox>


            <Button Name="Btn_PullDataAndCalculate" Grid.Column="8" Grid.Row="2"  Click="Btn_PullDataAndCalculate_Click" >
                <Image Source="..\Img\Sync.png"></Image>
            </Button>


            <TextBlock Grid.Row="1" Grid.Column="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12">Fee per 1000 nutrition</TextBlock>
            <TextBox Name="Tb_Nutrition_Fee" Grid.Row="2" Grid.Column="9" MaxLines="1" PreviewTextInput="Tb_Nutrition_Fee_PreviewTextInput" TextAlignment="Center" >750</TextBox>


            <TextBlock Grid.Row="1" Grid.Column="10" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12">Main Tree Spec</TextBlock>
            <TextBox Name="Tb_Spec_MainTree" Grid.Row="2" Grid.Column="10" MaxLines="1" PreviewTextInput="TextBox_PreviewTextInput" TextAlignment="Center">0</TextBox>


        </Grid>


        <ListBox   Background="#1E2126" HorizontalContentAlignment="Stretch"  Grid.Row="1" Margin="10,0,10,0" Name="Lb_Items" ItemContainerStyle="{DynamicResource ListBoxItemStyle1}"  ItemTemplate="{DynamicResource itemTemplateV2}" >
            

        </ListBox>




        <!--<Grid Grid.Row="1"  >
            <Grid.RowDefinitions>
                <RowDefinition Height="0.1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="0.1*"></RowDefinition>
            </Grid.RowDefinitions>

            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.1*"/>
                <ColumnDefinition Width="0.8*"/>
                <ColumnDefinition Width="2*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1.8*"/>
                <ColumnDefinition Width="0.1*"/>

            </Grid.ColumnDefinitions>

            --><!--<Grid Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="5" Background="#CD8C57">

            </Grid>

            <Grid Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="5" Background="#CD8C57">

            </Grid>

            <Grid Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="5" Background="#CD8C57">

            </Grid>

            <Grid Grid.Row="7" Grid.Column="1" Grid.ColumnSpan="5" Background="#CD8C57">

            </Grid>-->


            <!--//////////////// ITEM1 //////////////////--><!--


          
            <Grid ShowGridLines="True" Grid.Row="1" Grid.Column="1" Margin="2" Name="Grid_Item1" Background="Transparent" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                    <ColumnDefinition Width="2*"></ColumnDefinition>
                </Grid.ColumnDefinitions>

                <Image Name="Img_Item1" Grid.Row="0" Grid.Column="0" VerticalAlignment="Center"></Image>
                <TextBlock Name="Tb_Item1_Name" Grid.Row="0" Grid.Column="1"  VerticalAlignment="Top" Margin="0,35,0,0" ></TextBlock>
                <TextBlock Name="Tb_Item1_Price"  Grid.Row="0" Grid.Column="1" VerticalAlignment="Bottom" Margin="0,0,0,35" ></TextBlock>
            </Grid>


            <Grid ShowGridLines="True" Grid.Row="1" Grid.Column="2">

                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                    <ColumnDefinition Width="1*"></ColumnDefinition>

                </Grid.ColumnDefinitions>

                <Grid.RowDefinitions>
                    <RowDefinition Height="1*"></RowDefinition>
                    <RowDefinition Height="1*"></RowDefinition>

                </Grid.RowDefinitions>






                <Grid ShowGridLines="True" Grid.Row="0" Grid.Column="0" Margin="2" Name="Grid_Item1_1" Background="Transparent" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="4*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>


                    <Image Name="Img_Item1_Ing1" MaxHeight="50" MaxWidth="50" VerticalAlignment="Center" Grid.Column="0"></Image>

                    <TextBlock Name="Tb_Item1_Ing1" VerticalAlignment="Top" Grid.Column="1" Margin="0,5,0,0"></TextBlock>
                    <TextBlock Name="Tb_Item1_Ing1_Price" VerticalAlignment="Bottom" Grid.Column="1" Margin="0,0,0,5"></TextBlock>

                </Grid>




                <Grid ShowGridLines="True" Grid.Row="1" Grid.Column="0" Margin="2" Name="Grid_Item1_2" Background="Transparent" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="4*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>


                    <Image Name="Img_Item1_Ing2" MaxHeight="50" MaxWidth="50" VerticalAlignment="Center" Grid.Column="0"></Image>

                    <TextBlock Name="Tb_Item1_Ing2" VerticalAlignment="Top" Grid.Column="1" Margin="0,5,0,0"></TextBlock>
                    <TextBlock Name="Tb_Item1_Ing2_Price" VerticalAlignment="Bottom" Grid.Column="1" Margin="0,0,0,5"></TextBlock>

                </Grid>

                <Grid ShowGridLines="True" Grid.Row="0" Grid.Column="1" Margin="2" Name="Grid_Item1_3" Background="Transparent" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="4*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>


                    <Image Name="Img_Item1_Ing3" MaxHeight="50" MaxWidth="50" VerticalAlignment="Center" Grid.Column="0"></Image>

                    <TextBlock Name="Tb_Item1_Ing3" VerticalAlignment="Top" Grid.Column="1" Margin="0,5,0,0"></TextBlock>
                    <TextBlock Name="Tb_Item1_Ing3_Price" VerticalAlignment="Bottom" Grid.Column="1" Margin="0,0,0,5"></TextBlock>

                </Grid>

                <Grid ShowGridLines="True" Grid.Row="1" Grid.Column="1" Margin="2" Name="Grid_Item1_5" Background="Transparent" Visibility="Visible" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="4*"></ColumnDefinition>
